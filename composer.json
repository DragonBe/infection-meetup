{
    "name": "dragonbe/infection-meetup",
    "description": "A small PoC to explain what mutation testing is and why it's important to apply this on your current tests and source code to improve the quality of your code",
    "type": "project",
    "autoload": {
        "psr-4": {
            "DragonBe\\Meetup\\": "src/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload-dev": {
        "psr-4": {
            "DragonBe\\Test\\Meetup\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "^3.2",
        "fzaninotto/faker": "^1.7",
        "sensiolabs/security-checker": "^4.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michelangelo van Dam",
            "email": "dragonbe+github@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
